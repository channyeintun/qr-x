{
  "name": "@qr-x/react",
  "version": "0.0.1",
  "license": "MIT",
  "description": "",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "qr",
    "code",
    "react"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:storybook && yarn build:dts && yarn build:esm && yarn build:cjs",
    "build:dts": "tsc -p tsconfig.build.json --outDir ./dist",
    "build:esm": "esbuild index.tsx --bundle --packages=external --format=esm --outfile=./dist/index.esm.js",
    "build:cjs": "esbuild index.tsx --bundle --packages=external --format=cjs --outfile=./dist/index.cjs.js",
    "build:storybook": "PLATFORM=react storybook build -c ../../.storybook"
  },
  "dependencies": {
    "@qr-x/core": "*",
    "tsconfig": "*"
  },
  "peerDependencies": {
    "react": "^16.8.0  || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0  || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@storybook/blocks": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.2"
  }
}
