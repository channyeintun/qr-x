    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "esbuild": "^0.19.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^16.x || ^17.x || ^18.x",
    "react-dom": "^16.x || ^17.x || ^18.x"
  }
