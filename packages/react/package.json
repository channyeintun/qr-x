{
  "name": "@qr-x/react",
  "version": "0.0.1",
  "license": "MIT",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "qr",
    "code",
    "react"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:dts && yarn build:cjs && yarn build:src",
    "build:dts": "tsc -p tsconfig.build.json --outDir ./dist",
    "build:cjs": "esbuild index.tsx --format=cjs --outfile=./dist/index.js",
    "build:src": "esbuild tags/index.tsx tags/index.native.tsx --format=cjs --outdir=./dist/tags"
  },
  "dependencies": {
    "@qr-x/core": "^0.0.1"
  },
  "peerDependencies": {
    "react": "^16.x || ^17.x || ^18.x",
    "react-dom": "^16.x || ^17.x || ^18.x",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "@common/stories": "workspace:*",
    "@common/tsconfig": "workspace:*",
    "@storybook/react": "^7.6.4",
    "@storybook/react-vite": "^7.6.4",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "esbuild": "^0.19.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-svg": "^14.0.0",
    "typescript": "^5.3.3"
  }
}
