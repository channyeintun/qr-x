{
  "name": "@qr-x/react-native",
  "version": "0.0.1",
  "license": "MIT",
  "description": "",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "qr",
    "code",
    "react"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:dts && yarn build:esm && yarn build:cjs",
    "build:dts": "tsc -p tsconfig.build.json --outDir ./dist",
    "build:esm": "esbuild index.tsx --bundle --packages=external --format=esm --outfile=./dist/index.esm.js",
    "build:cjs": "esbuild index.tsx --bundle --packages=external --format=cjs --outfile=./dist/index.cjs.js"
  },
  "dependencies": {
    "@qr-x/core": "*",
    "tsconfig": "*"
  },
  "peerDependencies": {
    "react": "^16.x || ^17.x || ^18.x",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "@types/react": "^18.2.39",
    "esbuild": "^0.19.8",
    "react": "^18.2.0",
    "react-native": "^0.72.7",
    "react-native-svg": "^14.0.0",
    "typescript": "^5.3.2"
  }
}
