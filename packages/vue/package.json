{
  "name": "@qr-x/vue",
  "version": "0.0.1",
  "license": "MIT",
  "description": "",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "qr",
    "code",
    "vue"
  ],
  "scripts": {
    "build": "rm -rf dist && yarn build:dts && yarn build:esm && yarn build:cjs && yarn build:src",
    "build:dts": "tsc -p tsconfig.build.json --outDir ./dist",
    "build:esm": "esbuild index.ts --format=esm --outfile=./dist/index.esm.js",
    "build:cjs": "esbuild index.ts --format=cjs --outfile=./dist/index.cjs.js",
    "types:check": "tsc --skipLibCheck --noEmit"
  },
  "dependencies": {
    "@qr-x/core": "*"
  },
  "peerDependencies": {
    "vue": "^3.x"
  },
  "devDependencies": {
    "@storybook/test": "^7.6.4",
    "@storybook/vue3": "^7.6.4",
    "@storybook/vue3-vite": "^7.6.4",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-vue": "^4.5.2",
    "esbuild": "^0.19.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stories": "*",
    "tsconfig": "*",
    "vue": "^3.3.11"
  }
}
