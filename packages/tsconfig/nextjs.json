{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ES5",
    "allowJs": true,
    "incremental": true,
    "plugins": [{ "name": "next" }],
    "typeRoots": ["./types"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
