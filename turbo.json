{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "build:core": {
      "outputs": [".dist/**"],
      "dependsOn": ["@qr-x/core#build"]
    }
  }
}
